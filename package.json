{
  "name": "@dschau/gatsby-blog-starter-kit",
  "description": "Gatsby blog starter example",
  "version": "1.0.0",
  "author": "Dustin Schau <dustinschau@gmail.com>",
  "dependencies": {
    "gatsby": "~1.8.4",
    "gatsby-link": "~1.6.7",
    "gatsby-plugin-catch-links": "~1.0.4",
    "gatsby-plugin-manifest": "^1.0.3",
    "gatsby-plugin-offline": "^1.0.3",
    "gatsby-plugin-react-helmet": "~1.0.2",
    "gatsby-plugin-sass": "^1.0.2",
    "gatsby-plugin-sharp": "~1.6.1",
    "gatsby-plugin-styled-components": "^1.0.2",
    "gatsby-remark-autolink-headers": "^1.4.3",
    "gatsby-remark-copy-linked-files": "^1.5.1",
    "gatsby-remark-images": "~1.5.2",
    "gatsby-remark-prismjs": "^1.2.0",
    "gatsby-remark-smartypants": "^1.4.2",
    "gatsby-source-filesystem": "~1.4.2",
    "gatsby-transformer-remark": "~1.7.0",
    "gatsby-transformer-sharp": "^1.6.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-helmet": "^5.1.3",
    "react-icons": "~2.2.5",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "babel-eslint": "7",
    "cli-glob": "^0.1.0",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.0",
    "gh-pages": "~1.0.0",
    "prettier": "~1.5.3",
    "remark-cli": "^4.0.0",
    "remark-preset-lint-recommended": "^3.0.0",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-scss": "^2.0.1",
    "write-good": "^0.11.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean:public": "rm -rf public/*.html",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "deploy:surge": "yarn clean:public && gatsby build && surge public/",
    "develop": "gatsby develop",
    "preserve": "npm run build",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "format": "prettier --trailing-comma es5 --single-quote --write \"src/**/*.js\"",
    "test": "echo \"Should probably write some tests!\"",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern public,static .",
    "lint:md": "remark src/posts/",
    "lint:style": "stylelint src/**/*.css",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "write-good": "write-good $(glob 'src/posts/**/*.md')"
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
