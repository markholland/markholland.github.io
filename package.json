{
  "name": "partiallogic.com",
  "description": "Personal Site",
  "version": "1.0.0",
  "author": "Mark Holland <mark@partiallogic.com>",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "yarn clean && gatsby build",
    "clean": "rm -rf public/*.html",
    "deploy": "gatsby build && gh-pages -m ‘auto commit [ci skip]’ -d public",
    "deploy:surge": "surge --domain partiallogic.surge.sh public/",
    "develop": "gatsby develop",
    "preserve": "yarn run build",
    "serve": "gatsby serve",
    "start": "yarn run develop",
    "format": "prettier --trailing-comma es5 --single-quote --write \"src/**/*.jsx\"",
    "test": "echo \"Should probably write some tests!\"",
    "lint": "yarn lint:js && yarn lint:md && yarn lint:style",
    "lint:js": "eslint src/",
    "lint:md": "remark posts/",
    "lint:style": "stylelint src/**/*.css",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "write-good": "write-good $(glob 'src/posts/**/*.md')"
  },
  "dependencies": {
    "gatsby": "~1.9.240",
    "gatsby-link": "~1.6.39",
    "gatsby-plugin-catch-links": "~1.0.19",
    "gatsby-plugin-manifest": "^1.0.15",
    "gatsby-plugin-offline": "^1.0.15",
    "gatsby-plugin-react-helmet": "~2.0.8",
    "gatsby-plugin-sass": "^1.0.23",
    "gatsby-plugin-sharp": "~1.6.41",
    "gatsby-plugin-styled-components": "^2.0.9",
    "gatsby-remark-autolink-headers": "^1.4.13",
    "gatsby-remark-copy-linked-files": "^1.5.30",
    "gatsby-remark-images": "~1.5.59",
    "gatsby-remark-prismjs": "^1.2.20",
    "gatsby-remark-smartypants": "^1.4.12",
    "gatsby-source-filesystem": "~1.5.27",
    "gatsby-transformer-remark": "~1.7.37",
    "gatsby-transformer-sharp": "^1.6.22",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-icons": "~2.2.7",
    "styled-components": "^3.2.3"
  },
  "devDependencies": {
    "babel-eslint": "8",
    "cli-glob": "^0.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "gh-pages": "^1.1.0",
    "prettier": "~1.11.1",
    "remark-cli": "^5.0.0",
    "remark-preset-lint-recommended": "^3.0.1",
    "stylefmt": "^6.0.0",
    "stylelint": "^9.1.3",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^2.5.0",
    "surge": "^0.20.1",
    "write-good": "^0.11.3"
  },
  "keywords": [
    "gatsby"
  ],
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}