{
  "name": "partiallogic.com",
  "description": "Personal Site",
  "version": "1.0.0",
  "author": "Mark Holland <mark@partiallogic.com>",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "npm run clean && gatsby build",
    "clean": "rm -rf public/*.html",
    "deploy": "gatsby build && echo 'www.partiallogic.com' > ./public/CNAME && gh-pages -m ‘auto commit [ci skip]’ -d ./public",
    "deploy:surge": "npm run build && surge --domain partiallogic.surge.sh public/",
    "develop": "gatsby develop",
    "preserve": "npm run build",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "format": "prettier --trailing-comma es5 --single-quote --write \"src/**/*.jsx\"",
    "test": "echo \"Should probably write some tests!\"",
    "lint": "npm run lint:js && npm run lint:md && npm run lint:style",
    "lint:js": "eslint src/",
    "lint:md": "remark posts/",
    "lint:style": "stylelint src/**/*.css",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "write-good": "write-good $(glob 'src/posts/**/*.md')"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.18.21",
    "gatsby-link": "^2.2.28",
    "gatsby-plugin-catch-links": "^2.1.24",
    "gatsby-plugin-manifest": "^2.2.37",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sass": "^2.1.27",
    "gatsby-plugin-sharp": "^2.3.13",
    "gatsby-plugin-styled-components": "^3.1.17",
    "gatsby-remark-autolink-headers": "^2.1.23",
    "gatsby-remark-copy-linked-files": "^2.1.36",
    "gatsby-remark-images": "^3.1.42",
    "gatsby-remark-prismjs": "^3.3.30",
    "gatsby-remark-smartypants": "^2.1.20",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-remark": "^2.6.48",
    "gatsby-transformer-sharp": "^2.3.12",
    "node-sass": "^4.13.0",
    "prismjs": "^1.19.0",
    "prop-types": "^15.6.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.0",
    "react-icons": "~2.2.7",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "babel-eslint": "8",
    "cli-glob": "^0.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "gh-pages": "^1.1.0",
    "prettier": "^1.14.3",
    "remark-cli": "^5.0.0",
    "remark-preset-lint-recommended": "^3.0.1",
    "stylefmt": "^6.0.0",
    "stylelint": "^9.1.3",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^2.5.0",
    "surge": "^0.20.1",
    "write-good": "^0.11.3"
  },
  "keywords": [
    "gatsby"
  ],
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
