{
  "name": "partiallogic.com",
  "description": "Personal Site",
  "version": "1.0.0",
  "author": "Mark Holland <mark@partiallogic.com>",
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "npm run clean && gatsby build",
    "clean": "rm -rf public/*.html",
    "deploy": "gatsby build && echo 'www.partiallogic.com' > ./public/CNAME && gh-pages -m ‘auto commit [ci skip]’ -d ./public",
    "deploy:surge": "npm run build && surge --domain partiallogic.surge.sh public/",
    "develop": "gatsby develop",
    "preserve": "npm run build",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "format": "prettier --trailing-comma es5 --single-quote --write \"src/**/*.jsx\"",
    "test": "echo \"Should probably write some tests!\"",
    "lint": "npm run lint:js && npm run lint:md && npm run lint:style",
    "lint:js": "eslint .",
    "lint:md": "remark posts/",
    "lint:style": "stylelint src/**/*.scss",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "write-good": "write-good $(glob 'posts/**/*.md')"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^2.32.3",
    "gatsby-plugin-catch-links": "^2.10.0",
    "gatsby-plugin-manifest": "^2.12.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-sass": "^3.2.0",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-plugin-styled-components": "^3.10.0",
    "gatsby-remark-autolink-headers": "^2.11.0",
    "gatsby-remark-copy-linked-files": "^2.10.0",
    "gatsby-remark-images": "^3.11.0",
    "gatsby-remark-prismjs": "^3.13.0",
    "gatsby-remark-smartypants": "^2.10.0",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-transformer-remark": "^2.16.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "node-sass": "^5.0.0",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "~4.2.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "cli-glob": "^0.1.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "gh-pages": "^3.1.0",
    "prettier": "^2.2.1",
    "remark-cli": "^9.0.0",
    "remark-preset-lint-recommended": "^5.0.0",
    "stylefmt": "^6.0.3",
    "stylelint": "^13.10.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.19.0",
    "surge": "^0.21.7",
    "write-good": "^1.0.8"
  },
  "keywords": [
    "gatsby"
  ],
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "resolutions": {
    "@graphql-tools/merge@^6.2.8": "6.2.7"
  }
}
